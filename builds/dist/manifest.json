//This manifest is for adding icons to mobile users homescreens
{
  "name": "SVG Weather", // Nme or your site or app
  "short_name": "SVG Weather", // Name shown on user homescreen
  "icons": [{
        "src": "images/touch/icon-128x128.png",
        "sizes": "128x128",
        "type": "image/png"
      }, {
        "src": "images/touch/apple-touch-icon.png",
        "sizes": "152x152",
        "type": "image/png"
      }, {
        "src": "images/touch/ms-touch-icon-144x144.png",
